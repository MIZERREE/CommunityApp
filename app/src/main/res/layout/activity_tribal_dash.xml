<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".roles.tribal.TribalDashActivity"
    android:id="@+id/fragment_tribal_container">

    <com.ismaeldivita.chipnavigation.ChipNavigationBar
        android:id="@+id/bottom_nav_menu_tribal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cnb_menuResource="@menu/buttom_navigation_menu"
        android:background="@drawable/bottom_nav_round_conners"
        android:layout_alignParentBottom="true"
        app:cnb_unselectedColor="@color/black"
        app:cnb_badgeColor="@color/white"
        app:cnb_radius="8dp"
        app:cnb_orientationMode="horizontal"
        android:layout_margin="10dp"
        android:elevation="8dp"
        />


</RelativeLayout>